def romanos2(x):
  conversor={1000: 'M', 900: 'CM', 500: 'D', 400: 'CD', 100: 'C', 90: 'XC', 50: 'L', 40: 'XL', 10: 'X', 9: 'IX', 5: 'V', 4: 'IV', 1: 'I'}
  numero=''
  for valor, romano in conversor.items():
    while x >= valor:
      numero = numero + romano
      x = x - valor
  return numero   
romanos2(int(input()))   
